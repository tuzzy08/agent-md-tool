{
  "name": "agent-md-tool",
  "version": "1.0.0",
  "description": "CLI tool to download documentation and embed compressed indexes into AGENTS.md files",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "agent-md-tool": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "cli": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "agents",
    "ai",
    "documentation",
    "cli",
    "agents-md",
    "claude",
    "llm",
    "coding-agent"
  ],
  "author": {
    "name": "David Lawrence",
    "email": "tuzzy08@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuzzy08/agent-md-tool"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
